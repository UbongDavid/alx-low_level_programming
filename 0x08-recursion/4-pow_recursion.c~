#include "main.h"

/**
 * _puts_recursion - prints a string followed by a newline
 * @s: a string variable
 * Return: Void
 */

int _pow_recursion(int x, int y)
{
	if (y = 0)
	{
		return(1);
	}
	else if (y < 0)
	{
		return(-1)
	}
	else
	{
	x *= _pow_recursion(x, y)
		_putchar('\n');
                return;
	y++;
	}
}
