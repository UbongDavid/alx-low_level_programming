/**
 * *_memset - fills memory with a constant byte.
 * @s: pointer to put the constant
 * @b: constant
 * @n: max bytes to use
 * Return: s
 */

void _puts_recursion(char *s)
{
	int i = 0;

	(! *s)? return (0)
		/*while (*s){*/
	_putchar(*s++);
	i++;
	}
	putchar('\n');
	return i+1;
}
